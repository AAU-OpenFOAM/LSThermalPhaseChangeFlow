FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];
internalField   uniform 10;

boundaryField
{     
    down
    {
        type            zeroGradient;
    }
    FarStream
    {
	type           zeroGradient;
    }
    symmetryPlanes
    {
        type            symmetry;
    }
    frontAndBack
    {
        type            empty;
    }  
}
