FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];
internalField uniform 646;

boundaryField
{
    down
    {
        type            fixedValue;
        value           uniform 646;
    }
    FarStream
    {
        type            inletOutlet;
        inletValue      uniform 646;
        value           uniform 646;
    }
    symmetryPlanes
    {
        type            symmetry;
    }
    frontAndBack
    {
        type            empty;
    }   
}
