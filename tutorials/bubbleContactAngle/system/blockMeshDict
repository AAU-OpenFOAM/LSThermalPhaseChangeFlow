{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

scale 1;

D 128;


vertices
(
    (0 0 0) //0
    (.5 0 0) //1
    (.5 .5 0) //2
    (0 .5 0) //3
    (0 0 .5) //4
    (.5 0 .5)  //5
    (.5 .5 .5) //6
    (0 .5 .5) //7
);

blocks
(
    hex (0 1 2 3 4 5 6 7) ($D $D 1) simpleGrading (1 1 1)
);

edges
(
);

patches
(
    symmetry symmetryPlanes 
    (
        (0 4 7 3)
    )
    
    patch down
    (
        (1 5 4 0)
    )
    
    
    patch FarStream
    (
        (6 2 1 5)
        (7 6 2 3)
    )
    
    empty frontAndBack 
    (
        (0 3 2 1)
        (4 5 6 7)
    )



);

mergePatchPairs
(
