FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.water 0.0
);

regions        
(
    boxToCell
    {
        boxes
        (
            (0  0 0)
            (1e-5 2e-5 2e-6 )
        );
        fieldValues
        (
            volScalarFieldValue alpha.water 1.0
        );
    }
);


